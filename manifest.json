{
    "manifest_version": 3,
    "name": "Site to T-Share",
    "version": "1.0",
    "description": "provides a link to the matching thrillshare editing page",
    "permissions": ["activeTab"],
    "minimum_chrome_version": "102",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "action": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    }
